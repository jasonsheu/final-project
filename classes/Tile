public class Tile
{
    private int xVal, yVal;
    private boolean b;
    
    public Tile( int x, int y, boolean b )
    {
        xVal = x;
        yVal = y;
        this.b = b;
    }
    
    public int getX()
    {
        return xVal;
    }
    
    public int getY()
    {
        return yVal;
    }
    
    public void setX( int x )
    {
        xVal = x;
    }
    
    public void setY( int y )
    {
        yVal = y;
    }
    
    public boolean ifSprite()
    {
        return b;
    }
    
    public void setIfSprite( boolean b )
    {
        this.b = b;
    }
    
}
